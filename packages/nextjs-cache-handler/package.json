{
  "name": "@fortedigital/nextjs-cache-handler",
  "author": {
    "name": "Forte_ Digital",
    "url": "https://fortedigital.com"
  },
  "homepage": "https://github.com/fortedigital/nextjs-cache-handler",
  "bugs": "https://github.com/fortedigital/nextjs-cache-handler/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortedigital/nextjs-cache-handler.git"
  },
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "description": "Next.js cache handlers",
  "exports": {
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "composite": [
        "dist/composite.d.ts"
      ],
      "redis-strings": [
        "dist/redis-strings.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup --dts-resolve",
    "prepare": "npm run build",
    "clean": "rimraf ./dist ./.turbo ./node_modules",
    "dev": "tsup --watch",
    "test": "tsx --test src/**/*.test.ts",
    "test:watch": "tsx --watch --test src/**/*.test.ts",
    "format": "npx prettier --write .",
    "lint": "npx eslint",
    "lint:fix": "npm run format && npm run lint -- --fix"
  },
  "dependencies": {
    "@neshca/cache-handler": "^1.7.3",
    "cluster-key-slot": "1.1.2",
    "lru-cache": "11.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "22.7.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.10.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "2.5.2",
    "rimraf": "6.0.1",
    "tsup": "^8.3.0",
    "tsx": "4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "peerDependencies": {
    "next": ">=13.5.1",
    "redis": ">=4.6"
  },
  "distTags": [
    "next13.5",
    "next14"
  ],
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  },
  "files": [
    "dist"
  ]
}
